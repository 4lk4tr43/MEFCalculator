<Window x:Class="MEFCalculator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MEFCalculator" Height="350" Width="525" PreviewKeyDown="Window_PreviewKeyDown">
    <Grid>
        <TextBox Name="Display" Margin="10,10,75,0" VerticalAlignment="Top"/>
        <TextBox Name="Operation" Margin="447,10,10,0" VerticalAlignment="Top"/>
        <WrapPanel Name="Operations"  Margin="10,33,10,10"/>
    </Grid>
</Window>
